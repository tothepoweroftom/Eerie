{
  "name": "vanilla-parcel-boilerplate",
  "version": "1.0.0",
  "description": "Simple workflow starter for building vanilla JS apps",
  "main": "index.js",
  "scripts": {
    "dev": "npm run clean && parcel public/index.html --out-dir development -p 3000",
    "build": "parcel build public/index.html --out-dir dist --public-url ./",
    "clean": "rimraf ./development && rimraf -rf ./.cache"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bradtraversy/vanilla-parcel-boilerplate.git"
  },
  "keywords": [
    "parcel"
  ],
  "author": "Brad Traversy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bradtraversy/vanilla-parcel-boilerplate/issues"
  },
  "homepage": "https://github.com/bradtraversy/vanilla-parcel-boilerplate#readme",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/runtime-corejs2": "^7.9.2",
    "parcel-bundler": "^1.12.4",
    "rimraf": "^3.0.2",
    "sass": "^1.26.3"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "dist": "^0.1.2",
    "gsap": "^3.2.6",
    "jquery": "^3.5.0",
    "orbit-controls-es6": "^2.0.1",
    "parcel-plugin-static-files-copy": "^2.3.1",
    "postprocessing": "^6.13.3",
    "three": "^0.115.0",
    "three-gltf-loader": "^1.111.0",
    "three-orbit-controls": "^82.1.0",
    "tone": "^13.8.25"
  }
}
